<% if params[:member_id] == "new" %>
	Attendance.addRow("<%= j ({ 
		name: @this_member.name, 
		pretty_name: @this_member.pretty_name, 
		meeting_id: @meeting.id,
		id: @this_member.id
	}).to_json.html_safe %>")
<% end %>

Attendance.updateRoomCount(<%= @meeting.id %>, <%= @meeting.events.count %>)