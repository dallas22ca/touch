    //Yourube videos
    var tag = document.createElement('script');
    tag.src = "https://www.youtube.com/iframe_api";
    var firstScriptTag = document.getElementsByTagName('script')[0];
    firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);

    var players = [];
    var playerIds = [];
    var youtubeAPIReady = false;
    var player;

    function VideoNeedsToBeLoaded(id) {

        //if ($('#' + id).is(':visible')) {
            for (var index = 0; index < players.length; index++) {
                if (players[index].getVideoData().video_id == id) {
                    return false;
                }
            }
            return true;
        //}
        //return false;
    }


    function onYouTubeIframeAPIReady() {
        youtubeAPIReady = true;     
    }

    //The YouTube IFrame API will call this function when a video player is ready.
    function onPlayerReady(video) {

        players.push(video.target);

        if (!ApplicationSettings.IsMobile) {
            //video.target.playVideo();
        }
                
          
    }

    function onPlayerStateChange(event) {

        if (event.data == YT.PlayerState.PLAYING && event.target.gatracked != true) {
            event.target.gatracked = true;
            if (event.target.a.id == 'LIuWU1L_oh4') {
                _gaq.push(['_trackEvent', 'Home Page', 'Make sure you\'re ready to buy Video']);
            } else if (event.target.a.id == '1YjXVxv4A6w') {
                _gaq.push(['_trackEvent', 'Home Page', 'Decide what you want to buy Video']);
            } else if (event.target.a.id == 'MrgpiV1cvPw') {
                _gaq.push(['_trackEvent', 'Home Page', 'Figure out how much you can afford Video']);
            } else if (event.target.a.id == 'hJIyHUT4TBo') {
                _gaq.push(['_trackEvent', 'Home Page', 'Determine your home\'s asking price Video']);
            } else if (event.target.a.id == 'vjdwzi_Xdkg') {
                _gaq.push(['_trackEvent', 'Home Page', 'Find a REALTORÂ® who is right for you Video']);
            } else if (event.target.a.id == 'O-3kgC5jYEM') {
                _gaq.push(['_trackEvent', 'Home Page', 'Decide when to sell Video']);
            }
        }
    }

    function stopVideos() {

        if (ApplicationSettings.IsMobile) {
            return;
        }
        if (players) {
            for (var index = 0; index < players.length; index++) {
                players[index].stopVideo();
            }
        }
    }

    function PlayVideo(step) {
        if (!ApplicationSettings.IsMobile) {
            var sectionNum = step.split('_')[0];
            var videoId = $('#m_buysellguide_header_' + sectionNum).next('iframe').attr('id');

            for (var index = 0; index < players.length; index++) {
                if (videoId == players[index].getVideoData().video_id) {
                    players[index].seekTo(0, true);
                    players[index].playVideo();

                } else {
                    players[index].stopVideo();
                }
            }
        }
    }





