    <!-- ROYAL SLIDER -->
    <script src="Presentation/Javascript/frameworks/jquery.easing.1.3.min.js" type="text/javascript"></script>
    <script src="Presentation/Javascript/frameworks/royal-slider-8.1.min.js" type="text/javascript"></script>


<script type="text/javascript">
    

  
    // CREATE GLOBAL VARIABLE TO HOLD DOCUMENT READY
    $.indexReady = function () {

        

        //LoadVisibleUnloadedVideos();

        //IE retains values on back, but the drop kicks aren't updated - force to first element
        resetSearchFilters();
        if (CookieHandler.getCookieValueAsString(GlobalData.REALTOR_HOME_SHOW_ADVANCED) != "1") {
            $('#m_hme_fltr_params_row3').hide();
        } else {
            $("#btnMoreOptions").html("<span>Fewer Options</span>");
        }

    
        $("#NumberofDays").datepicker(
          {
              showOn: "button",
              showButtonPanel: true,
              numberOfMonths: 2,
              maxDate: 0,
              buttonImage: "Presentation/Images/common/icons/dropkick_arrow_black_dwn.png",
              buttonImageOnly: true,
              dateFormat: "m/d/yy",
              minDate: -363,
              beforeShow: function (input, inst) {
            
                      $(this).parent().children('input').css('background', '#ffffff');
                      $(this).parent().children('input').css('border-top', '1px solid #C9C9C4');
                      $(this).parent().children('input').css('border-right', '1px solid #C9C9C4');
                      $(this).parent().children('input').css('border-bottom', 'none');
                      $(this).parent().children('input').css('border-left', '1px solid #C9C9C4');
                      $(this).parent().children('.ui-datepicker-trigger').attr('src', 'Presentation/Images/common/icons/calendar_close.png');
                      setTimeout(function () {
                          var buttonPane = $(input).datepicker("widget").find(".ui-datepicker-buttonpane");
                          $("<button>",
                              {
                                  text: "Clear Date", click: function () {
                                      $.datepicker._clearDate(input);
                                  }
                              }).appendTo(buttonPane);
                      }, 1);
                  
              },
              onClose: function (dateText, inst) {
                  //if (!isMapDrawing) {
                  $(this).parent().children('input').css('background', '#f1f1eb');
                  $(this).parent().children('input').css('border', '1px solid #C9C9C4');
                  $(this).parent().children('.ui-datepicker-trigger').attr('src', 'Presentation/Images/common/icons/dropkick_arrow_black_dwn.png');
                  //}
              },
              onSelect: function () {

                  if ($(this).parent().children('input').val() == "") {
                      $(this).parent().children('input').val("Listed Since")
                  }   
              }
          }
      );

        $('#NumberofDays').focus(function () {
            if (!$("#NumberofDays").datepicker("widget").is(":visible")) {
                $('#NumberofDays').datepicker('show');
            } 
        });
        $('.m_hme_fltr_listed_since img').click(function () {

            if (!$("#NumberofDays").datepicker("widget").is(":visible")) {

                $('#NumberofDays').datepicker('show');
            }
        });
        $("#NumberofDays").val("Listed Since");

        $('.m_hme_return').click(function () {
            window.location = '';
        });


        if (CookieHandler.getCookieValueAsArray(GlobalData.PRE_VIEWED_MLS_COOKIE) != '') {  
            makeAjaxCall();
            if (ApplicationSettings.CultureID === '1' && 'true' == 'true') {
                $('#homeAdverts').show();
            }
            $(window).on('hashchange', function (e) {
                LoadPreviouslyViewed();
            });
        } else {
            $('#recentlyViewed').hide();

            if (ApplicationSettings.CultureID === '1' && 'true' == 'true') {
                $('#homeAdverts').show();
            } else {
                $('#welcome').show();
            }


          
        }
      
        function ResetAdvancedSearchFields()
        {
            $('#m_hme_fltr_params_row3 select').each(function () {
                                                              
                $(this).prop('selectedIndex', 0).dropkick('setValue', $("#" + $(this).attr('id') + " option:selected").val());

                if ($(this).attr('title')) {
                    $('#dk_label_' + $(this).attr('id')).html($(this).attr('title'));
                }
            });

            $('#m_hme_fltr_params_row3 :checkbox').each(function () {
                $(this).attr("checked", false);
            });  
        }

        $("#btnMoreOptions").click(function (event) {
            if ($('#m_hme_fltr_params_row3').is(":visible")) {
                CookieHandler.deleteCookie(GlobalData.REALTOR_HOME_SHOW_ADVANCED);

                $('#m_hme_fltr_params_row3').hide();
                $("#btnMoreOptions").html("<span>More Options</span>");                

                ResetAdvancedSearchFields();
                _gaq.push(['_trackEvent', 'Home Page', 'Fewer Options Button']);

            } else {
                CookieHandler.createCookie(GlobalData.REALTOR_HOME_SHOW_ADVANCED, "1");
                $('#m_hme_fltr_params_row3').show();
                $("#btnMoreOptions").html("<span>Fewer Options</span>");
                _gaq.push(['_trackEvent', 'Home Page', 'More Options Button']);
            }

        
        });

        $(".youtubeVideo").click(function (event) {
      

            if (VideoNeedsToBeLoaded(this.id)) {
                player = new YT.Player(this.id, {
                    width: '850',
                    height: '515',
                    videoId: this.id,   
                    playerVars: {
                        modestbranding: '1',
                        theme: 'light',
                        showinfo: '0',
                        rel: '0',                        
                        enablejsapi: '1'
                    },
                    events: {
                        'onReady': onPlayerReady,
                        'onStateChange': onPlayerStateChange
                    }
                });

                playerIds.push(this.id);
             
            }
        });

        $("#bottomPrevious").click(function (event) {
            event.preventDefault ? event.preventDefault() : event.returnValue = false;
            
            if (HashSearch.keyExists("Page")) {
                var page = Number(HashSearch.get('Page'));
                HashSearch.set('Page', page - 1);
            }
        });

      

        $("#bottomNext").click(function (event) {
            event.preventDefault ? event.preventDefault() : event.returnValue = false;            
            
            if (HashSearch.keyExists("Page")) {
                var page = Number(HashSearch.get('Page'));
                HashSearch.set('Page', page + 1);
            } else {
                HashSearch.set('Page', '2');
            }
        });

    
        // If user clicks on the enter button when typing in a search parameter we will start the search
        $(".m_hme_srch_ipt_txtbox").keyup(function (e) {
            if(e.keyCode == 13){
                e.preventDefault();
                StartSearch();
            }
        });        

        $('#button_realtor_go').click(function (e) {
            e.preventDefault();
            StartSearch();
        });

        $('#home_search_input').keypress(function (e) {
            
            if (e.which == 13 || e.keyCode == 13) {
                
                $('#button_realtor_go').trigger('click');
                return false;
            }
        });

       

        if ($('#royalslider-def').is(":visible")) {
            $('#royalslider-def').royalSlider({
                controlNavThumbs: false,
                imageAlignCenter: false,
                directionNavEnabled: true,
                welcomeScreenEnabled: false,
                hideArrowOnLastSlide: true
            });
        }
      
    };

    function GetPreiouslyViewedIds() {

        var previouslyViewed = CookieHandler.getCookieValueAsArray(GlobalData.PRE_VIEWED_MLS_COOKIE);
        var ids = '';
        for (var index = previouslyViewed.length; index >= 0; index--) {
            if (previouslyViewed[index] && previouslyViewed[index] != '')
                ids += previouslyViewed[index] + ",";
        }

        if (ids.length > 0) {
            ids = ids.substring(0, ids.length - 1);
        }
        
        return ids;
    }
        

    function makeAjaxCall() {

        var theQuery = "ApplicationId=" + ApplicationSettings.ApplicationId + "&cultureid=" + ApplicationSettings.CultureID + "&RecordsPerPage=" + GlobalData.MAX_PRE_MLS + "&Listingids=" + GetPreiouslyViewedIds();

        if (theQuery) {
            AjaxEngine.getResults(ApplicationSettings.ApiPath + "Listing.svc/PropertySearch_Post", theQuery, loadResults, 1, 'POST');
        }
    }

    function loadResults(results) {

        allServerRsults = results;

        LoadPreviouslyViewed();
    }

    function LoadPreviouslyViewed() {

        pageStr = '';
        if (!allServerRsults)
            return;

        var page = Number(HashSearch.get('Page', 1));

        favCookieEndIndex = (GlobalData.MAX_PRE_MLS_PER_PAGE * page);
        favCookieStartIndex = favCookieEndIndex - (GlobalData.MAX_PRE_MLS_PER_PAGE);

        RenderResults(allServerRsults);
                
        event.preventDefault ? event.preventDefault() : event.returnValue = false;
        return true;      
    }

    function RenderResults(results) {
        
        var resultMatched = false;
        var totalFavListLength = null;
        var resultsHTML = '';
        var IDArray = GetPreiouslyViewedIds().split(",");

        activatePager(results.Results.length);
        
        var count = 0;
        var actualResults = [];
        for (var index = 0; index < IDArray.length; index++) {

            var id = IDArray[index].split('_')[0];
            var result = null;

            for (var rindex = 0; rindex < results.Results.length; rindex++) {

                if (results.Results[rindex].Id == id) {
                    actualResults.push(results.Results[rindex]);                  
                    break;
                }
            }          
        }

        for (var index = favCookieStartIndex; index < favCookieEndIndex; index++) {

            if (actualResults[index]) {
                resultsHTML += RenderListing(actualResults[index], index % 2);
            }
        }

   

        $('#recentlyViewedListings').html('');
        $('#recentlyViewedListings').html(resultsHTML);

        if (actualResults.length > 4) {
            $('.m_rslt_ftr').show();
        } else {
            $('.m_rslt_ftr').hide();
        }

        // Hide loading
        $('.m_load_icon').slideUp();
        searchInAction = false;
    }

    function RenderListing(result, property_cell_count) {
        var propertyImage = '';
        var propertyBedsBaths = [];
        var propertyDetails = [];
        var propertyPrice = [];
        var organizationDesignation = '';
        var listings = '';

        (result.Property.Photo != null) ? propertyImage = result.Property.Photo[0].MedResPath : propertyImage = 'Presentation/Images/common/placeholders/no-image.png';

        switch (result.Property.TypeId) {
            case '308':
                var parking = '';
                $.each(result.Property.Parking, function (i, val) {
                    parking += val.Name + ' ';
                });
                propertyDetails.push('<span>' + parking + '</span><br />');
                break;
            case '302':
                if (typeof (result.Property.FarmType) != 'undefined') { propertyDetails.push('<span>' + result.Property.FarmType + '</span><br />') };
                if (typeof (result.Land.SizeTotal) != 'undefined') { propertyDetails.push('<span>' + result.Land.SizeTotal + '</span><br />') };
                if (typeof (result.Building.Bedrooms) != 'undefined') { propertyBedsBaths.push('<span>' + result.Building.Bedrooms + '</span><img src="Presentation/Images/common/icons/bed.png" alt="' + Message.Bedrooms + '" title=" ' + Message.Bedrooms + '"/>') };
                if (typeof (result.Building.BathroomTotal) != 'undefined') { propertyBedsBaths.push('<span>' + result.Building.BathroomTotal + '</span><img src="Presentation/Images/common/icons/bath.png" alt="' + Message.Bathrooms + '" title="' + Message.Bathrooms + '"/>') };
                break;
            case '303':
                if (typeof (result.Land.CurrentUse) != 'undefined') { propertyDetails.push('<span>' + result.Land.CurrentUse + '</span><br />') };
                if (typeof (result.Land.SizeTotal) != 'undefined') { propertyDetails.push('<span>' + result.Land.SizeTotal + '</span><br />') };
                break;
            default:
                if (typeof (result.Property.Type) != 'undefined') { propertyDetails.push('<span id="hm_lst_proptype' + (property_cell_count + 1) + '">' + result.Property.Type + '</span>') };
                if (typeof (result.Building.Type) != 'undefined') { propertyDetails.push('<span>' + result.Building.Type + '</span>') };
                if (typeof (result.Building.Bedrooms) != 'undefined') { propertyBedsBaths.push('<span id="hm_lst_bed_num' + (property_cell_count + 1) + '">' + result.Building.Bedrooms + '</span><img  id="hm_lst_bed_img' + (property_cell_count + 1) + '" src="Presentation/Images/common/icons/bed.png" alt="' + Message.Bedrooms + '" title=" ' + Message.Bedrooms + '"/>') };
                if (typeof (result.Building.BathroomTotal) != 'undefined') { propertyBedsBaths.push('<span id="hm_lst_bath_num' + (property_cell_count + 1) + '">' + result.Building.BathroomTotal + '</span><img  id="hm_lst_bath_img' + (property_cell_count + 1) + '" src="Presentation/Images/common/icons/bath.png" alt="' + Message.Bathrooms + '" title="' + Message.Bathrooms + '"/>') };
                break;
        }

        // Organization Designation
        if (typeof (result.Individual[0].Organization.Designation) != 'undefined') { organizationDesignation = result.Individual[0].Organization.Designation };

        (result.Property.Price) ? propertyPrice.push(result.Property.Price) : null;
        (result.Property.LeaseRent) ? propertyPrice.push(result.Property.LeaseRent) : null;

        var favState = $.inArray(result.Id + '_' + result.Property.Address.Latitude + '_' + result.Property.Address.Longitude, CookieHandler.getCookieValueAsArray(GlobalData.REALTOR_FAVORITES_COOKIE)) > -1 ? '_selected' : '_unselected';
        var comState = $.inArray(result.Id, CookieHandler.getCookieValueAsArray(GlobalData.REALTOR_COMPARE_COOKIE)) > -1 ? '_selected' : '_unselected';

        if (property_cell_count == 0) {
            listings += '<div class="m_gallery_lst_row">';
        }

        listings += '<div class="m_gallery_lst_hme_cell">';

        //listings += '   <div class="m_gallery_lst_cell_sec m_gallery_lst_hme_cell_first">';

        if (result.AlternateURL && result.AlternateURL.DetailsLink) {
            listings += '   <a href="' + result.AlternateURL.DetailsLink + '" onclick="TrackAltFeatureSheet(\'' + result.Id + '\'); return true;" target="_blank">';
        } else {
            listings += '   <a href="propertyDetails.aspx?PropertyId=' + result.Id + '">';
        }

        
        
        listings += '       <div class="m_gallery_lst_cell_hdr">';
        listings += (result.OpenHouse ? '<span class="m_gallery_lst_cell_hdr_opnhouse">' + propertyResultsPanel.OpenHouse + '</span>' : '');
        listings += '       </div>';
        listings += '       <div class="m_gallery_lst_cell_sec">';
        listings += '           <div class="m_gallery_lst_cell_img">';
        listings += '               <img id="hm_lst_image' + (property_cell_count + 1) + '" src="' + propertyImage + '" alt="' + result.Property.Address.AddressText + '" />';
        listings += '           </div>';
        listings += '           <div id="hm_lst_address' + (property_cell_count + 1) + '"  class="m_gallery_lst_cell_address clickable">';
        listings += result.Property.Address.AddressText.replace('|', '<br />');
        listings += '           </div>';
        listings += '           </div>';
        listings += '       </a>';
       // listings += '   </div>';
        listings += '   <div class="m_gallery_lst_cell_sec m_gallery_lst_cell_sec_info">';
        listings += '       <div class="m_gallery_lst_cell_row">';
        listings += '           <div class="m_gallery_lst_cell_lft">';
        listings += '               <div id="hm_lst_price' + (property_cell_count + 1) + '"  class="m_gallery_lst_cell_price">' + (result.Property.Price || result.Property.LeaseRent) + '</div>';
        listings += '           </div>';
        listings += '           <div class="m_gallery_lst_cell_rgt">';
        listings += '               <div class="m_gallery_lst_cell_bed_bath">' + propertyBedsBaths.join('') + '</div>';
        listings += '           </div>';
        listings += '       </div>';
        listings += '       <div class="m_gallery_lst_cell_row">';
        listings += '           <div class="m_gallery_lst_cell_lft">';
        listings += '               <div class="m_gallery_lst_cell_type">' + propertyDetails.join(' ') + '</div>';
        listings += '           </div>';
        listings += '       </div>';
        listings += '       <div class="m_gallery_lst_cell_row">';
        listings += '           <div class="m_gallery_lst_cell_lft">';
        listings += '               <div id="hm_lst_MLSID' + (property_cell_count + 1) + '"  class="m_gallery_lst_cell_listing">' + propertyResultsPanel.ListingID + ' ' + result.MlsNumber + '</div>';
        listings += '           </div>';
        listings += '           <div class="m_gallery_lst_cell_rgt">';
        listings += '               <div class="m_gallery_lst_cell_tools">';
        listings += '                   <a href="#" id="hm_lst_comp' + (property_cell_count + 1) + '" onclick="AddToCompare(' + result.Id + ', this); return false;">' + getCompareImage(result.Id, comState) + '</a>';
        listings += '                   <a href="#" id="hm_lst_fav' + (property_cell_count + 1) + '" onclick="AddToFavorites(' + result.Id + ',' + result.Property.Address.Latitude + ',' + result.Property.Address.Longitude + ', this); return false;"> ' + getFavouriteImage(result.Id, favState) + '</a>';
        listings += '               </div>';
        listings += '           </div>';
        listings += '       </div>';
        listings += '   </div>';
        listings += '   <div class="m_gallery_lst_cell_ftr">';
        listings += '       <span><a href=\'OfficeDetails.aspx?OrganizationId=' + result.Individual[0].Organization.OrganizationID + '\'>' + result.Individual[0].Organization.Name + '</a><br/>' + organizationDesignation + '</span>';
        listings += '   </div>';

        listings += '</div>';

        

        if (property_cell_count == 1) {
            listings += '</div>';            
        }

        return listings;
    }





    function IsSearchCriteriaBlank() {

        return $('#PriceMin').val() == "0" &&
        $('#PriceMax').val() == "0" &&
        $('#NumberofDays').val() == "0" &&
        $('#BathRange').val() == "0-0" &&
        $('#BedRange').val() == "0-0" &&
        $('#home_search_input').val() == '';
        
    }

    //Front end validation of basic search elements
    function ValidateSearch() {

        var errorMessage = '';       
        
        //Sale price range
        if ($('#TransactionTypeID').val() == 2) {
            if (parseInt($('#PriceMax').val()) < parseInt($('#PriceMin').val()) && parseInt($('#PriceMax').val()) != 0) {
                errorMessage = Message.SalePriceRange;
            }
        }

        //Rent price range
        if ($('#TransactionTypeID').val() == 3) { 
            if (parseInt($('#RentMax').val()) < parseInt($('#RentMin').val()) && parseInt($('#RentMax').val()) != 0) {
                errorMessage = Message.RentPriceRange;
            }
        }

        //Open house range 
        if ($('#PropertyTypeID').val() === 'ohs' &&  $('#OpenHouseEndDate').val() != '') {
            if ($('#OpenHouseStartDate').val() == '' && $('#OpenHouseEndDate').val() != '') {
                //errorMessage = Message.InvalidDateRange;
            }
            else if ($('#OpenHouseStartDate').datepicker('getDate').getTime() > $('#OpenHouseEndDate').datepicker('getDate').getTime()) {
                errorMessage = Message.InvalidDateRange;
            }
        }
        
        
     
        if (errorMessage != '') {        
            showMessage(errorMessage);
            return false;
        }

        return true;
    }


    function resetSearchFilters() {
        $('#PropertyTypeID').prop('selectedIndex', 0).dropkick('setValue', $('#PropertyTypeID').val());
        $('#TransactionTypeID').prop('selectedIndex', 0).dropkick('setValue', $('#TransactionTypeID').val());
        $('#BedRange').prop('selectedIndex', 0).dropkick('setValue', $('#BedRange').val());
        $('#BathRange').prop('selectedIndex', 0).dropkick('setValue', $('#BathRange').val());
        $('#LandSizeRange').prop('selectedIndex', 0).dropkick('setValue', $('#LandSizeRange').val());
        $('#FarmTypeID').prop('selectedIndex', 0).dropkick('setValue', $('#FarmTypeID').val());
        $('#ParkingSpacesRange').prop('selectedIndex', 0).dropkick('setValue', $('#ParkingSpacesRange').val());
        $('#PriceMin').prop('selectedIndex', 0).dropkick('setValue', $('#PriceMin').val());
        $('#RentMin').prop('selectedIndex', 0).dropkick('setValue', $('#RentMin').val());
        $('#PriceMax').prop('selectedIndex', 0).dropkick('setValue', $('#PriceMax').val());
        $('#RentMax').prop('selectedIndex', 0).dropkick('setValue', $('#RentMax').val());
        $('#CropTypeId').prop('selectedIndex', 0).dropkick('setValue', $('#CropTypeId').val());
        $('#LiveStockTypeId').prop('selectedIndex', 0).dropkick('setValue', $('#LiveStockTypeId').val());
        $('#BuildingTypeId').prop('selectedIndex', 0).dropkick('setValue', $('#BuildingTypeId').val());
        $('#OwnershipTypeGroupId').prop('selectedIndex', 0).dropkick('setValue', $('#OwnershipTypeGroupId').val());
        $('#StoreyRange').prop('selectedIndex', 0).dropkick('setValue', $('#StoreyRange').val());
        $('#BedRangeOption').prop('selectedIndex', 0).dropkick('setValue', $('#BedRangeOption').val());
        $('#BathRangeOption').prop('selectedIndex', 0).dropkick('setValue', $('#BathRangeOption').val());
        $('#LandUseTypeId').prop('selectedIndex', 0).dropkick('setValue', $('#LandUseTypeId').val());
        $('#ViewTypeGroupId').prop('selectedIndex', 0).dropkick('setValue', $('#ViewTypeGroupId').val());
        $('#ParkingTypeId').prop('selectedIndex', 0).dropkick('setValue', $('#ParkingTypeId').val());
        $('#home_search_input').val('');

        $('#AirCondition, #Pool, #Fireplace, #Garage, #Waterfront, #Acreage, #OpenHouse').attr('checked', false);
    }

    function activatePager(totalRecords) {


        var currentPage = Number(HashSearch.get('Page', 1));

        // Pagination, cell and html variables        
        totalPages = numPages(totalRecords, GlobalData.MAX_PRE_MLS_PER_PAGE);

        if (currentPage <= totalPages) {

            $("#numberOfPagedRecordsBottom").show();
            // create the html for the pager
            $("#numberOfPagedRecordsBottom").html(currentPage + ' of ' + totalPages);

            // set buttons active or deactive for the apger
            ControlSimplePagerState(totalPages, { Next: '#bottomNext', Previous: '#bottomPrevious' }, currentPage);

            // show pager
            $(".m_rslt_hdr, .m_rslt, .m_rslt_ftr, .m_pager").fadeIn();
        } else {
            $("#numberOfPagedRecordsBottom").hide();
        }
    }


    function AddControlValToQuery(paramName, controlId) {
        var returnVal = '';

        if ($('#' + controlId).is(":visible") || $('#dk_label_' + controlId).is(":visible")) {
            switch ($('#' + controlId).prop('type')) {

                case "select-one":
                    if ($('#' + controlId).val() != "0") {
                        returnVal = "&" + paramName + "=" + $('#' + controlId).val();
                    }
                    break;
                case "checkbox":
                    if ($('#' + controlId).prop('checked')) {
                        returnVal = "&" + paramName + "=" + $('#' + controlId).val();
                    }
                    break;

                case "text":
                    if ($('#' + controlId).hasClass('hasDatepicker') && !/[a-zA-Z]+/.test($('#' + controlId).val())) {
                        try {
                            var d = new Date();
                            returnVal = "&" + paramName + "=" + Math.ceil((d.getTime() - $('#' + controlId).datepicker('getDate').getTime()) / ((1000 * 60 * 60 * 24)));
                        } catch (ex) { }
                    }
                    break;

            }
        }

        return returnVal;
    }

    function StartSearch() {      
       

        if (IsSearchCriteriaBlank()) {
            redirectToDisclaimer('Map.aspx');
        }

        if (!ValidateSearch()) {      
            return;
        }

        var areaSearch = new SubAreaSearch();
        areaSearch.input = $('#home_search_input').val(); // INPUT TEXTBOX
        areaSearch.searchType = 'FILTERS'; // BASIC || FILTERS

        areaSearch.searchCriteriaCommon += 'CultureId=' + ApplicationSettings.CultureID;
        areaSearch.searchCriteriaCommon += '&ApplicationId=' + ApplicationSettings.ApplicationId;

        areaSearch.searchFilters += AddControlValToQuery('BedRange', 'BedRangeOption');
        areaSearch.searchFilters += AddControlValToQuery('BathRange', 'BathRangeOption');
        areaSearch.searchFilters += AddControlValToQuery('BuildingTypeId', 'BuildingTypeId');
        areaSearch.searchFilters += AddControlValToQuery('ViewTypeGroupId', 'ViewTypeGroupId');
        areaSearch.searchFilters += AddControlValToQuery('OwnershipTypeGroupId', 'OwnershipTypeGroupId');
        areaSearch.searchFilters += AddControlValToQuery('LiveStockTypeId', 'LiveStockTypeId');
        areaSearch.searchFilters += AddControlValToQuery('CropTypeId', 'CropTypeId');
        areaSearch.searchFilters += AddControlValToQuery('StoreyRange', 'StoreyRange');
        areaSearch.searchFilters += AddControlValToQuery('FarmTypeId', 'FarmTypeId');
        areaSearch.searchFilters += AddControlValToQuery('LandUseTypeId', 'LandUseTypeId');
        areaSearch.searchFilters += AddControlValToQuery('ParkingTypeId', 'ParkingTypeId');
        areaSearch.searchFilters += AddControlValToQuery('AirCondition', 'AirCondition');
        areaSearch.searchFilters += AddControlValToQuery('Pool', 'Pool');
        areaSearch.searchFilters += AddControlValToQuery('Fireplace', 'Fireplace');
        areaSearch.searchFilters += AddControlValToQuery('Garage', 'Garage');
        areaSearch.searchFilters += AddControlValToQuery('Waterfront', 'Waterfront');
        areaSearch.searchFilters += AddControlValToQuery('Acreage', 'Acreage');
        areaSearch.searchFilters += AddControlValToQuery('NumberofDays', 'NumberofDays'); 
        
        if ($('#OpenHouse').is(":checked") && $('#OpenHouse').is(":visible")) {

            var today = new Date();
            var dd = today.getDate();
            var mm = today.getMonth() + 1; //January is 0!

            var yyyy = today.getFullYear();
            if (dd < 10) { dd = '0' + dd }
            if (mm < 10) { mm = '0' + mm }
            today = mm + '/' + dd + '/' + yyyy;

            areaSearch.searchFilters += "&OpenHouse=1";
            areaSearch.searchFilters += "&OpenHouseStartDate=" + today;
            //areaSearch.searchFilters += "&OpenHouseEndDate=" + today;
        }



        switch ($('#PropertyTypeID').val()) {
            case '300': case '301': case 'con': case 'ohs': //RESIDENTIAL, RECREATIONAL
                if ($('#PropertyTypeID').val() === '300' || $('#PropertyTypeID').val() === '301') { areaSearch.searchFilters += '&PropertyTypeId=' + $('#PropertyTypeID').val(); }
                if ($('#PropertyTypeID').val() === 'con') { areaSearch.searchFilters += '&OwnershipTypeGroupId=2' }                
                areaSearch.searchFilters += '&TransactionTypeId=' + $('#TransactionTypeID').val();

                switch ($('#TransactionTypeID').val()) {
                    case '1': //FOR SALE & RENT
                        areaSearch.searchFilters += '&PriceMin=' + $('#PriceMin').val();
                        areaSearch.searchFilters += '&PriceMax=' + $('#PriceMax').val();
                        areaSearch.searchFilters += '&RentMin=' + $('#RentMin').val();
                        areaSearch.searchFilters += '&RentMax=' + $('#RentMax').val();
                        break;
                    case '2': //FOR SALE
                        areaSearch.searchFilters += '&PriceMin=' + $('#PriceMin').val();
                        areaSearch.searchFilters += '&PriceMax=' + $('#PriceMax').val();
                        break;
                    case '3': //FOR RENT
                        areaSearch.searchFilters += '&RentMin=' + $('#RentMin').val();
                        areaSearch.searchFilters += '&RentMax=' + $('#RentMax').val();
                        break;
                }

                if ($('#PropertyTypeID').val() === '300' || $('#PropertyTypeID').val() === '301' || $('#PropertyTypeID').val() === 'con') {
                    areaSearch.searchFilters += '&BedRange=' + $('#BedRange').val();
                    areaSearch.searchFilters += '&BathRange=' + $('#BathRange').val();
                }                

                break;
            case '302': case '303': //AGRICULTURE, VACANT LAND
                areaSearch.searchFilters += '&PropertyTypeId=' + $('#PropertyTypeID').val();
                areaSearch.searchFilters += '&TransactionTypeId=' + $('#TransactionTypeID').val();

                switch ($('#TransactionTypeID').val()) {
                    case '1': //FOR SALE & RENT
                        areaSearch.searchFilters += '&PriceMin=' + $('#PriceMin').val();
                        areaSearch.searchFilters += '&PriceMax=' + $('#PriceMax').val();
                        areaSearch.searchFilters += '&RentMin=' + $('#RentMin').val();
                        areaSearch.searchFilters += '&RentMax=' + $('#RentMax').val();
                        break;
                    case '2': //FOR SALE
                        areaSearch.searchFilters += '&PriceMin=' + $('#PriceMin').val();
                        areaSearch.searchFilters += '&PriceMax=' + $('#PriceMax').val();
                        break;
                    case '3': //FOR RENT
                        areaSearch.searchFilters += '&RentMin=' + $('#RentMin').val();
                        areaSearch.searchFilters += '&RentMax=' + $('#RentMax').val();
                        break;
                }

                areaSearch.searchFilters += '&LandSizeRange=' + $('#LandSizeRange').val();

                if ($('#PropertyTypeID').val() === '302') { areaSearch.searchFilters += '&FarmTypeId=' + $('#FarmTypeID').val(); }
                

                break;
            case '308': //PARKING
                areaSearch.searchFilters += '&PropertyTypeId=' + $('#PropertyTypeID').val();
                areaSearch.searchFilters += '&TransactionTypeId=' + $('#TransactionTypeID').val();

                switch ($('#TransactionTypeID').val()) {
                    case '1': //FOR SALE & RENT
                        areaSearch.searchFilters += '&PriceMin=' + $('#PriceMin').val();
                        areaSearch.searchFilters += '&PriceMax=' + $('#PriceMax').val();
                        areaSearch.searchFilters += '&RentMin=' + $('#RentMin').val();
                        areaSearch.searchFilters += '&RentMax=' + $('#RentMax').val();
                        break;
                    case '2': //FOR SALE
                        areaSearch.searchFilters += '&PriceMin=' + $('#PriceMin').val();
                        areaSearch.searchFilters += '&PriceMax=' + $('#PriceMax').val();
                        break;
                    case '3': //FOR RENT
                        areaSearch.searchFilters += '&RentMin=' + $('#RentMin').val();
                        areaSearch.searchFilters += '&RentMax=' + $('#RentMax').val();
                        break;
                }

                areaSearch.searchFilters += '&ParkingSpacesRange=' + $('#ParkingSpacesRange').val();
                areaSearch.searchFilters += AddControlValToQuery('ParkingTypeId', 'ParkingTypeId');

                break;
        }


        if (areaSearch.input != '') {
            areaSearch.searchCriteria += '&Area=' + encodeURIComponent(areaSearch.input);
            areaSearch.Search();

            $('#home_search_input').focus();
        } else {
            if (areaSearch.searchFilters.substring(0, 1) == '&')
                areaSearch.searchFilters = areaSearch.searchFilters.substring(1);
            redirectToDisclaimer("Map.aspx#" + areaSearch.searchFilters);
        }



     
    }


    (function showRecentlyViewed() {

        var cookieValueArray = new Array();
        var recentlyViewedArray = new Array();
        var counter = 0;
        var cookieValue = null;

        // populate the array for recently viewed properties (recentlyViewedArray), with the correct number of properties viewed
        getMaxPre_MLS_Listing();

        // if there was at least one previously viewed property, then show it on the page
        if (recentlyViewedArray[0]) {
            CreateRecentlyViewedListings(recentlyViewedArray);
        }

        // this function gets the values for each listing_id from the serverf and creates the recently viewed property list
        function CreateRecentlyViewedListings(mlsArray) {
            for (var i = 0; i < mlsArray.length; i++) {
                // alert(mlsArray[i]);
            }
            // alert('All recently viewed properies created 4');
        }

        // gets the max number of MLS allowed from the recently viewed cookie
        function getMaxPre_MLS_Listing() {
            cookieValue = CookieHandler.getCookieValueAsArray("recentlyViewedMls")

            // there is no mls number in the cookie, then exit
            if (!cookieValue) {
                return;
            }

            // populate the recentlyview cookie with at least maximum previously viewed cookies
            for (var i = cookieValue.length - 2; i >= 0; i--) {
                if (counter >= GlobalData.MAX_PRE_MLS) {
                    break;
                }
                recentlyViewedArray.push(cookieValue[i]);
                counter++;
            }
        }

    } ());

    $("#OpenHouseStartDate, #OpenHouseEndDate").datepicker(
        {
            showOn: "button",
            showButtonPanel: true,
            numberOfMonths: 2,
            minDate: 0,
            buttonImage: "Presentation/Images/common/icons/dropkick_arrow_black_dwn.png",
            buttonImageOnly: true,
            dateFormat: "mm/dd/yy",
            beforeShow: function (input, inst) {
                $(this).parent().children('input').css('background', '#ffffff');
                $(this).parent().children('input').css('border-top', '1px solid #908d99');
                $(this).parent().children('input').css('border-right', '1px solid #908d99');
                $(this).parent().children('input').css('border-bottom', 'none');
                $(this).parent().children('input').css('border-left', '1px solid #908d99');
                $(this).parent().children('.ui-datepicker-trigger').attr('src', 'Presentation/Images/common/icons/calendar_close.png');
                setTimeout(function() 
                {
                    var buttonPane = $( input ).datepicker( "widget" ).find( ".ui-datepicker-buttonpane" );
                    $( "<button>", 
                        { text: 'Clear Date', click: function() {
                            $.datepicker._clearDate( input );
                        }
                    }).appendTo( buttonPane );
                }, 1 );
            },
            onClose: function (dateText, inst) {
                $(this).parent().children('input').css('background', '#f1f1eb');
                $(this).parent().children('input').css('border', '1px solid #dfdfd9');
                $(this).parent().children('.ui-datepicker-trigger').attr('src', 'Presentation/Images/common/icons/dropkick_arrow_black_dwn.png');
            }
        },
        $.datepicker.regional["en-CA"]
    );

