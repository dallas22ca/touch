            <script type="text/javascript">
jQuery.support.cors = true;
$.ajaxSetup({ cache: false });
function logAnalytics(listingId,eventType) {
if (listingId!=''){
var url = 'http://analytics.crea.ca/LogEvents.svc/LogEvents?&DestinationID=384&UUID=af0ad467-1ae1-4b69-9f01-82a41af9af1b&IP=142.167.151.203';
url += '&Listingid=' + listingId;
url += '&EventType=' + eventType;
 var req = $.ajax({url:url,crossDomain:true,dataType:'jsonp',type:'get',timeout:5000});
 req.error(function() {var xhr=req;if(xhr && xhr.readystate != 4){xhr.abort();}});
}}
</script>


