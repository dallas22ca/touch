<script type="text/javascript">

    function showSellersTab() {
        $('#howrealtorshelp_home_sellers_tab_con').css('position', 'static').css('top', '');
        $('#howrealtorshelp_home_buyers_tab_con').css('position', 'absolute').css('top', '-1000px');
        $('#howrealtorshelp_home_buyers_tab').removeClass('m_hme_advert_tab_sel');
        $('#howrealtorshelp_home_sellers_tab').addClass('m_hme_advert_tab_sel');
        
    }

    function showBuyersTab()
    {
        $('#howrealtorshelp_home_buyers_tab_con').css('position', 'static').css('top', '');
        $('#howrealtorshelp_home_sellers_tab_con').css('position', 'absolute').css('top', '-1000px');
        $('#howrealtorshelp_home_buyers_tab').addClass('m_hme_advert_tab_sel');
        $('#howrealtorshelp_home_sellers_tab').removeClass('m_hme_advert_tab_sel');
    }
</script>
        
