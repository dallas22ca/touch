    var MaxPriceFilterChange = function () {

        var maxVal = $("#PriceMax option:selected").val();

        $('#dk_container_PriceMin ul.dk_options_inner li a').each(function (index, item) {
            var entry = $(item);
            if (Number(entry.attr('data-dk-dropdown-value')) > maxVal && maxVal != 0) {
                entry.parent().hide();
            } else {
                entry.parent().show();
            }           
        });
    }

    var MinPriceFilterChange = function () {
        
        var minVal = $("#PriceMin option:selected").val();

        $('#dk_container_PriceMax ul.dk_options_inner li a').each(function (index, item) {
            var entry = $(item);
            if (Number(entry.attr('data-dk-dropdown-value')) < minVal && Number(entry.attr('data-dk-dropdown-value')) != 0){
                entry.parent().hide();
            } else {
                entry.parent().show();
            }           
        });
    }

    var MaxRentFilterChange = function () {

        var maxVal = $("#RentMax option:selected").val();

        $('#dk_container_RentMin ul.dk_options_inner li a').each(function (index, item) {
            var entry = $(item);
            if (Number(entry.attr('data-dk-dropdown-value')) > maxVal && maxVal != 0) {
                entry.parent().hide();
            } else {
                entry.parent().show();
            }
        });
    }

    var MinRentFilterChange = function () {

        var minVal = $("#RentMin option:selected").val();

        $('#dk_container_RentMax ul.dk_options_inner li a').each(function (index, item) {
            var entry = $(item);
            if (Number(entry.attr('data-dk-dropdown-value')) < minVal && Number(entry.attr('data-dk-dropdown-value')) != 0) {
                entry.parent().hide();
            } else {
                entry.parent().show();
            }
        });
    }

